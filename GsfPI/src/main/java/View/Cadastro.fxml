<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane alignment="CENTER" style="-fx-background-color: #386b8f;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CadastroController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #386b8f;">
         <children>
            <TextField fx:id="txtNomeCompleto" layoutX="210.0" layoutY="127.0" prefHeight="29.0" prefWidth="191.0" promptText="Nome Completo" />
            <Button fx:id="btnCadastrar" layoutX="250.0" layoutY="316.0" mnemonicParsing="false" onAction="#onClickCadastrar" prefHeight="25.0" prefWidth="100.0" text="Cadastrar" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="190.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../resources/imagens/gsf.png" />
               </image>
            </ImageView>
            <TextField fx:id="txtEmail" layoutX="210.0" layoutY="164.0" prefHeight="29.0" prefWidth="191.0" promptText="Email" />
            <PasswordField fx:id="txtSenha" layoutX="210.0" layoutY="271.0" prefHeight="29.0" prefWidth="191.0" promptText="Senha" />
            <Label layoutX="113.0" layoutY="133.0" text="Nome Completo" textFill="WHITE" />
            <Label layoutX="173.0" layoutY="170.0" text="Email" textFill="WHITE" />
            <Label layoutX="174.0" layoutY="206.0" text="CNPJ" textFill="WHITE" />
            <Label layoutX="158.0" layoutY="241.0" text="Telefone" textFill="WHITE" />
            <Label layoutX="171.0" layoutY="277.0" text="Senha" textFill="WHITE" />
            <Button fx:id="btnSair" layoutX="256.0" layoutY="351.0" mnemonicParsing="false" onAction="#onClickSair" prefHeight="25.0" prefWidth="89.0" text="Sair" />
            <TextField fx:id="txtCnpj" layoutX="210.0" layoutY="200.0" prefHeight="29.0" prefWidth="191.0" promptText="CNPJ" />
            <TextField fx:id="txtTelefone" layoutX="210.0" layoutY="235.0" prefHeight="29.0" prefWidth="191.0" promptText="Telefone" />
         </children>
      </AnchorPane>
   </children>
</FlowPane>
