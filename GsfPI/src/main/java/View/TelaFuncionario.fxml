<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>


<FlowPane alignment="CENTER" style="-fx-background-color: #386b8f;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.TelaFuncionarioController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #386b8f;">
         <children>
            <Button fx:id="btnVisualizarFuncionario" layoutX="70.0" layoutY="125.0" mnemonicParsing="false" onAction="#OnClickVisuFuncionario" prefHeight="150.0" prefWidth="190.0" text="Visualizar">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Button>
            <Button fx:id="btnCadastrarFuncionario" layoutX="348.0" layoutY="125.0" mnemonicParsing="false" onAction="#OnClickCadastrarFuncionario" prefHeight="150.0" prefWidth="190.0" text="Cadastrar">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</FlowPane>
